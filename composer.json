{
    "name": "chabok-php",
    "description": "Chabok Push Client for PHP",
    "version": "1.1.0",
    "keywords": [
        "chabok",
        "php",
        "sdk",
        "api"
    ],
    "homepage": "http://chabokpush.com",
    "license": "proprietary",
    "authors": [{
        "name": "ChabokPush",
        "homepage": "https://github.com/chabokpush/chabok-php-demo"
    }],
    "require": {
        "php": ">=5.4",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8",
        "satooshi/php-coveralls": "~1.0",
        "squizlabs/php_codesniffer": "~2.6",
        "friendsofphp/php-cs-fixer": "~1.12"
    },
    "autoload": {
        "psr-4": { "Chabok\\Client\\": "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "Chabok\\Client\\": "test/" }
    }
}